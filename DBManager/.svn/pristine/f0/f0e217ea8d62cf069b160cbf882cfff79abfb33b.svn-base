namespace DBManager
{
    /// <summary>
    /// Знак условия в запросе
    /// </summary>
    public enum QueryConditionSigns
    {
        /// <summary>
        /// Не определён (по умолчанию)
        /// </summary>
        UnKnown,
        /// <summary>
        /// Равно =
        /// </summary>
        Equals,
        /// <summary>
        /// НЕ равно !=
        /// </summary>
        NotEquals,
        /// <summary>
        /// Сравнение на NULL IS NULL
        /// </summary>
        EqulsNULL,
        /// <summary>
        /// Сравнение на NULL IS NOT NULL
        /// </summary>
        NotEqualsNull,
        /// <summary>
        /// Больше 
        /// </summary>
        MoreThan,
        /// <summary>
        /// Меньше 
        /// </summary>
        LessThan,
        /// <summary>
        /// Больше или равно
        /// </summary>
        MoreOrEquals,
        /// <summary>
        /// Меньше или равно
        /// </summary>
        LessOrEquals

    }
}
